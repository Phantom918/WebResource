package cn.picc.model;

import java.math.BigDecimal;
import java.util.Date;

public class Reward {
    private BigDecimal pkRewPun;  //奖励主键

    private BigDecimal pkEmp;  //员工主键 

    private BigDecimal pkOrg;  //

    private String rpType;  //

    private Date rpDate;  //奖励时间

    private String rpVasue;  //奖励事由

    private String rpGist;  //奖励名称

    private String rpMeasure;  //奖励内容

    private Date rpUndoTime;  //

    private String rpUndoCasue;  //

    private String status;  //状态 ：1为奖励， 2为惩罚

    private String remark;  //

    private Date modifyTime;  //

    public BigDecimal getPkRewPun() {
        return pkRewPun;
    }

    public void setPkRewPun(BigDecimal pkRewPun) {
        this.pkRewPun = pkRewPun;
    }

    public BigDecimal getPkEmp() {
        return pkEmp;
    }

    public void setPkEmp(BigDecimal pkEmp) {
        this.pkEmp = pkEmp;
    }

    public BigDecimal getPkOrg() {
        return pkOrg;
    }

    public void setPkOrg(BigDecimal pkOrg) {
        this.pkOrg = pkOrg;
    }

    public String getRpType() {
        return rpType;
    }

    public void setRpType(String rpType) {
        this.rpType = rpType;
    }

    public Date getRpDate() {
        return rpDate;
    }

    public void setRpDate(Date rpDate) {
        this.rpDate = rpDate;
    }

    public String getRpVasue() {
        return rpVasue;
    }

    public void setRpVasue(String rpVasue) {
        this.rpVasue = rpVasue;
    }

    public String getRpGist() {
        return rpGist;
    }

    public void setRpGist(String rpGist) {
        this.rpGist = rpGist;
    }

    public String getRpMeasure() {
        return rpMeasure;
    }

    public void setRpMeasure(String rpMeasure) {
        this.rpMeasure = rpMeasure;
    }

    public Date getRpUndoTime() {
        return rpUndoTime;
    }

    public void setRpUndoTime(Date rpUndoTime) {
        this.rpUndoTime = rpUndoTime;
    }

    public String getRpUndoCasue() {
        return rpUndoCasue;
    }

    public void setRpUndoCasue(String rpUndoCasue) {
        this.rpUndoCasue = rpUndoCasue;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }

    public Date getModifyTime() {
        return modifyTime;
    }

    public void setModifyTime(Date modifyTime) {
        this.modifyTime = modifyTime;
    }
}